{
  "private": true,
  "scripts": {
    "start": "export NODE_OPTIONS=--openssl-legacy-provider && umi dev",
    "build": "export NODE_OPTIONS=--openssl-legacy-provider && umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "docker:build": "docker build --platform linux/amd64 -f frontend/Dockerfile -t 3d-print .",
    "docker:run": "docker rm -f my-react-app || true && docker run -d -p 8080:80 --name my-react-app react-frontend --network=reaction-net"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/pro-components": "^2.8.7",
    "@ant-design/pro-layout": "^6.38.22",
    "@tweenjs/tween.js": "^25.0.0",
    "@umijs/plugin-sass": "^1.1.1",
    "axios": "^1.9.0",
    "babel-plugin-import": "^1.13.8",
    "body-parser": "^2.2.0",
    "cors": "^2.8.5",
    "dayjs": "1.11.4",
    "postcss-px-to-viewport": "^1.1.1",
    "react": "17.x",
    "react-dom": "17.x",
    "styled-components": "^6.1.17",
    "three": "^0.178.0",
    "umi": "^3.5.43"
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/three": "^0.178.1",
    "@umijs/max": "^4.4.11",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.43",
    "file-loader": "^6.2.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "sass": "^1.89.0",
    "typescript": "^4.1.2",
    "url-loader": "^4.1.1",
    "yorkie": "^2.0.0"
  }
}
